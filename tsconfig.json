{
  "compilerOptions": {
    "declaration": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": [
      "ESNext",
      "dom"
    ],
    "types": [
      "@types/node",
      "@cloudflare/workers-types"
    ]
  },
  "include": [
    "scripts",
    "src",
    "test"
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}
